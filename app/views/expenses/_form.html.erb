<%= form_for @expense do |f| %>
  <div class="field">
    <%= f.label :date %>
    <%= f.date_field :date %>
  </div>
  <div class="field">
    <%= f.label :expense_type %>
    <%= f.text_field :expense_type %>
  </div>
  <div class="field">
    <%= f.label :amount %>
    <%= f.text_field :amount %>
  </div>
  <div class="field">
    <%= f.label :notes %>
    <%= f.text_field :notes %>
  </div>
  <%= f.submit %>
<% end %>
